{
  "VERSION_FILES": [
    {
      "file": "README.md",
      "pattern": "- Version v(\\d+\\.\\d+\\.\\d+)",
      "template": "- Version v{version}"
    },
    {
      "file": "package.json",
      "pattern": "\"version\": \"(\\d+\\.\\d+\\.\\d+)\"",
      "template": "\"version\": \"{version}\""
    },
    {
      "file": "*.py",
      "pattern": "version = \"v(\\d+\\.\\d+\\.\\d+)\"",
      "template": "version = \"v{version}\""
    }
  ]
}